@page "/annotate"
@using Blazor.Extensions.Canvas;
@using Blazor.Extensions.Canvas.Canvas2D;
@using Blazor.Extensions;
@using System.Numerics;

<h3>Annotating</h3>

<div @onmousedown=@BeginDrawing
     @onmouseup=@EndDrawingAsync
     @onmousemove=@ContinueDrawingAsync
     style="background-image: url('@DOG'); 
     background-repeat: no-repeat">
    <BECanvas Width="460" Height="292" @ref="@canvasReference"></BECanvas>
</div>

@code {
    private const string DOG = "data:image/png;base64,";

    private Canvas2DContext? context;
    private BECanvasComponent? canvasReference;

    private AnnotationInProgress? currentAnnotation;

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        context = await canvasReference.CreateCanvas2DAsync();
        await context.SetStrokeStyleAsync("green");
        await context.SetLineWidthAsync(2);
    }

    private async Task DrawCurrentAnnotationAsync(AnnotationInProgress annotationInProgress)
    {
        var x = annotationInProgress.BeginPoint.X;
        var y = annotationInProgress.BeginPoint.Y;

        var targetX = annotationInProgress.EndPoint.X;
        var targetY = annotationInProgress.EndPoint.Y;

        await context!.BeginBatchAsync();
        //await context!.ClearRectAsync(0, 0, 460, 292);
        await context!.BeginPathAsync();
        await context!.MoveToAsync(x, y);
        await context!.LineToAsync(targetX, targetY);
        await context!.StrokeAsync();
        await context!.EndBatchAsync();
    }

    private void BeginDrawing(MouseEventArgs e)
    {
        var x = (float)e.OffsetX;
        var y = (float)e.OffsetY;

        currentAnnotation = new()
        {
            BeginPoint = new(x, y)
        };
    }

    private async Task ContinueDrawingAsync(MouseEventArgs e)
    {
        return;
        if (currentAnnotation is not { })
        {
            return;
        }

        var x = (float)e.OffsetX;
        var y = (float)e.OffsetY;

        var asfoasfoa = new AnnotationInProgress()
        {
            BeginPoint = currentAnnotation.BeginPoint,
            EndPoint = new(x, y)
        };

        await DrawCurrentAnnotationAsync(asfoasfoa);
    }
    
    private async Task EndDrawingAsync(MouseEventArgs e)
    {
        var x = (float)e.OffsetX;
        var y = (float)e.OffsetY;

        currentAnnotation!.EndPoint = new(x, y);
        await DrawCurrentAnnotationAsync(currentAnnotation);
        currentAnnotation = null;
    }

    private class AnnotationInProgress
    {
        public Vector2 BeginPoint { get; set; }
        public Vector2 EndPoint { get; set; }
    }
}
